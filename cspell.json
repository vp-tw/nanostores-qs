{
  "allowCompoundWords": true,
  "enableFiletypes": ["*"],
  "enableGlobDot": true,
  "files": ["**/*"],
  "ignorePaths": [
    "**/*.svg",
    "**/.env.*.local",
    "**/.env.local",
    "**/.gitignore",
    "**/__snapshots__/**",
    "/*.code-workspace",
    "/.changeset",
    "/.git",
    "/pnpm-lock.yaml"
  ],
  "includeRegExpList": ["[ -~]"],
  "useGitignore": true,
  "version": "0.2",
  "words": [
    "antfu",
    "astrojs",
    "baseui",
    "commitizen",
    "fontsource",
    "jetbrains",
    "jsdoc",
    "nanostores",
    "nojekyll",
    "nums",
    "nvmrc",
    "oxfmt",
    "oxfmtrc",
    "pathe",
    "plex",
    "popstate",
    "starlight",
    "tsdoc",
    "tsgo",
    "vdustr",
    "vite",
    "vitejs",
    "vitest"
  ]
}
