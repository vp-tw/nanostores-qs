{
  "allowCompoundWords": true,
  "enableFiletypes": ["*"],
  "enableGlobDot": true,
  "files": ["**/*"],
  "ignorePaths": [
    "**/*.svg",
    "**/.env.*.local",
    "**/.env.local",
    "**/.gitignore",
    "**/__snapshots__/**",
    "/*.code-workspace",
    "/.changeset",
    "/.git",
    "/pnpm-lock.yaml"
  ],
  "includeRegExpList": ["[ -~]"],
  "useGitignore": true,
  "version": "0.2",
  "words": [
    "antfu",
    "commitizen",
    "jsdoc",
    "nanostores",
    "nums",
    "nvmrc",
    "oxfmt",
    "oxfmtrc",
    "pathe",
    "tsdoc",
    "tsgo",
    "vite",
    "vitejs",
    "vitest"
  ]
}
